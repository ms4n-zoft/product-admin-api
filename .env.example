# Server Configuration
PORT=3000

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/your-database

# JWT Configuration
# IMPORTANT: Change this to a secure random string in production!
# Generate one using: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your-super-secret-jwt-key-change-in-production-use-64-chars-minimum
JWT_EXPIRES_IN=24h

# Admin Password (email must be @zoftwarehub.com domain)
# IMPORTANT: Change this in production!
ADMIN_PASSWORD=change-this-secure-password
